@model MyPersonalWebsite.Models.Classes.Icons
@{
    ViewBag.Title = "NewIcon";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Add New Icon</h2>
<form class="form-group" method="post">
    @Html.LabelFor(x => x.IconName)
    @Html.TextBoxFor(x => x.IconName, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Link)
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
<br />
<button class="btn btn-success">Add Icon</button>


</form>

