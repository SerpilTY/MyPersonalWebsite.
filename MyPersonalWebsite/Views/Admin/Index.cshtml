@using MyPersonalWebsite.Models.Classes
@model List<HomePage> 

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered mt-3 ">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Update</th>
    </tr>
    @foreach (var item in Model)
    {     
        <tr>
            <td>@item.Id</td>
            <td>@item.UserName</td>
            <td><a href="#" class="btn btn-primary">Update</a></td>
        </tr>
     }
        
    </table>


